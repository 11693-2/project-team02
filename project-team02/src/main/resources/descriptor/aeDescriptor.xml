<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>  
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="QuestionAnnotatorDescriptor">
      <import name="descriptor.QuestionAnnotatorDescriptor"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DocumentAnnotatorDescriptor">
      <import name="descriptor.DocumentAnnotatorDescriptor"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="SnippetDescriptor">
      <import name="descriptor.SnippetDescriptor"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>aeDescriptor</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback"/>
